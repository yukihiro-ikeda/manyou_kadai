<h2>管理者用ユーザ編集</h2>
<%= form_with(model: [:admin, @user], local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.label :admin %>
  <%= f.check_box :admin %>
  <%= f.submit "Update" %>
<% end %>
<%= link_to "管理一覧へ戻る",admin_users_path %>